{
  "name": "nx-electron-node-react-boilerplate",
  "productName": "NX Electron Node React Boilerplate",
  "version": "0.0.0",
  "description": "A foundation for scalable desktop apps",
  "author": "Nimble",
  "license": "MIT",
  "main": "./release/build/electron/main.js",
  "private": true,
  "scripts": {
    "dev": "npx nx run-many -t serve -p renderer electron",
    "test:unit": "NODE_ENV=test npx nx run-many -t test -p renderer api components",
    "test:e2e": "E2E='true' npx nx e2e e2e --skipInstall",
    "lint:api": "npx nx lint api",
    "lint:electron": "npx nx lint electron",
    "lint:renderer": "npx nx lint renderer",
    "lint:components": "npx nx lint components",
    "lint:all": "npx nx run-many -t lint -p api electron renderer components",
    "build:electron": "npx nx build:prod electron --verbose",
    "build:renderer": "npx nx build renderer --prod",
    "build:app": "yarn clean-release-build && yarn build:electron && yarn build:renderer",
    "package:electron-app:mac": "electron-builder build --mac --publish never",
    "package:electron-app:win": "npx electron-builder@22.14.13 --win --publish never",
    "clean-release-build": "rm -rf ./release/build ./release/out",
    "commit": "git-cz",
    "prepare": "husky install",
    "postinstall": "npx husky install && electron-builder install-app-deps"
  },
  "dependencies": {
    "@swc/helpers": "~0.5.2",
    "axios": "^1.1.0",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@commitlint/prompt-cli": "^17.6.1",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@nx/cypress": "17.1.3",
    "@nx/devkit": "17.1.3",
    "@nx/eslint": "17.1.3",
    "@nx/eslint-plugin": "17.1.3",
    "@nx/express": "17.1.3",
    "@nx/jest": "17.1.3",
    "@nx/js": "17.1.3",
    "@nx/node": "17.1.3",
    "@nx/playwright": "17.1.3",
    "@nx/react": "17.1.3",
    "@nx/vite": "17.1.3",
    "@nx/webpack": "17.1.3",
    "@nx/workspace": "17.1.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/decompress": "^4.2.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "~4.0.0",
    "@vitest/coverage-v8": "~0.32.0",
    "@vitest/ui": "~0.32.0",
    "babel-jest": "^29.4.1",
    "commitizen": "^4.3.0",
    "dotenv": "^16.0.3",
    "electron": "^27.0.4",
    "electron-builder": "^24.9.1",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-environment-node": "^29.4.1",
    "jest-ratchet": "^1.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "~22.1.0",
    "nx": "17.1.3",
    "nx-electron": "^16.1.0",
    "prettier": "^2.6.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.55.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-jest-mock-import-meta": "^1.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "~5.2.2",
    "vite": "~4.3.9",
    "vite-plugin-dts": "~2.3.0",
    "vitest": "~0.32.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.8.0"
  },
  "build": {
    "files": [
      "release/build",
      "node_modules",
      "package.json",
      ".config/buildResources/**/*"
    ],
    "directories": {
      "buildResources": "./.config/buildResources",
      "output": "release/out"
    },
    "extends": null,
    "publish": {
      "provider": "github",
      "owner": "Nimble",
      "repo": "nx-electron-node-react-boilerplate"
    },
    "productName": "Nimble Electron Boilerplate",
    "appId": "nimble.desktop-app.nx-node-react-boilerplate",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "afterSign": ".config/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": ".config/entitlements.mac.plist",
      "entitlementsInherit": ".config/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "yarn@4.0.2"
}
